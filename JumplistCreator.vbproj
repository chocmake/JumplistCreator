  <Import Project="$(MSBuildToolsPath)\Microsoft.VisualBasic.targets" />
  <Target Name="GenerateAssemblyInfoFromTemplate" BeforeTargets="CoreCompile">
    <MakeDir Directories="$(IntermediateOutputPath)" Condition="!Exists('$(IntermediateOutputPath)')" />

    <ReadLinesFromFile File="Templates\AssemblyInfo.template">
      <Output TaskParameter="Lines" ItemName="AsmTemplateLines" />
    </ReadLinesFromFile>

    <!-- Replace placeholders and write generated file -->
    <WriteLinesToFile
      File="$(GeneratedAssemblyInfo)"
      Lines="@(AsmTemplateLines->Replace('$ProductName$','$(CanonicalProductName)')->Replace('$AssemblyVersion$','$(CanonicalAssemblyVersion)')->Replace('$FileVersion$','$(CanonicalFileVersion)')->Replace('$Copyright$','$(CanonicalCopyright)')->Replace('$Guid$','$(CanonicalGuid)'))"
      Overwrite="true" />

    <!-- Include output for compilation -->
    <ItemGroup>
      <Compile Include="$(GeneratedAssemblyInfo)" />
    </ItemGroup>
  </Target>
  <Target Name="GenerateManifest" BeforeTargets="PrepareResources">
      <Message Text='Generating manifest at $(GeneratedManifest)' Importance='High' />
      <MakeDir Directories="$(IntermediateOutputPath)" Condition="!Exists('$(IntermediateOutputPath)')" />
      <ReadLinesFromFile File="Templates\app.manifest.template">
          <Output TaskParameter="Lines" ItemName="ManifestTemplateLines" />
      </ReadLinesFromFile>
      <WriteLinesToFile
          File="$(GeneratedManifest)"
          Overwrite="true"
          Lines="@(ManifestTemplateLines->Replace('$ProductName$','$(CanonicalProductName)')->Replace('$FileVersion$','$(CanonicalFileVersionPadded)'))" />
  </Target>
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <PropertyGroup>
      <DistributionDir>$(SolutionDir)dist\</DistributionDir>
      <LanguagesSourceDir>$(ProjectDir)Languages\</LanguagesSourceDir>

      <ThemeDll>$(ProjectDir)libs\bin\Theme.dll</ThemeDll>
      <CompiledBin>$(TargetDir)$(TargetFileName)</CompiledBin>
      <InsertIconsBin>$(ProjectDir)tools\InsertIcons.exe</InsertIconsBin>
    </PropertyGroup>

    <ItemGroup>
      <LanguageFiles Include="$(LanguagesSourceDir)*.txt"
                     Exclude="$(LanguagesSourceDir)en-US.txt" />
    </ItemGroup>

    <!-- Embed icons -->
    <Exec Command='"$(InsertIconsBin)" "$(CompiledBin)" "$(ProjectDir)Resources\Icons"' />

    <!-- Copy/create specific files/directories for distribution -->
    <MakeDir Directories="$(DistributionDir)" />
    <!-- Only create Languages dir if language(s) other than default exist -->
    <MakeDir Directories="$(DistributionDir)Languages"
             Condition="'@(LanguageFiles)' != ''" />
     
    <Copy SourceFiles="$(CompiledBin)" 
          DestinationFolder="$(DistributionDir)" />

    <Copy SourceFiles="$(ThemeDll)" 
          DestinationFolder="$(DistributionDir)"
          Condition="Exists('libs\bin\Theme.dll')" />

    <Copy SourceFiles="@(LanguageFiles)" 
          DestinationFolder="$(DistributionDir)Languages"
          Condition="'@(LanguageFiles)' != ''" />
  </Target>

